#define NULL 0
#define O_RDONLY 0

#define PROT_READ 1
#define MAP_PRIVATE 2

let fd = syscall(2, "tests/cat.pasic", O_RDONLY, 0)
let stat = mem
syscall(5, fd, stat)

let fsize = *(stat + 48)
let file = syscall(9, NULL, fsize, PROT_READ, MAP_PRIVATE, fd, 0)

let i = 0
while i < fsize do
    let value = *(file + i)
    let j = 0
    while j < 8 do 
        let char = (value >> (j*8)) & 255
        // print(char)
        write(char, 1)
        j = j + 1
    end
    i = i + 8
end
